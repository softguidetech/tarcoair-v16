        <record model="ir.ui.view" id="request_inbound_reconcile_kanban_view">
            <field name="name">request inbound Kanban</field>
            <field name="model">request.inbound</field>
            <field name="arch" type="xml">
                <kanban class="o_res_partner_kanban" create="true">
                    <field name="name"/>
                    <field name="user_name"/>
                    <field name="request_date"/>
                    <field name="amount"/>
                    <templates>
                        <t t-name="kanban-box">
                            <div class="oe_kanban_global_click">
                                <div class="o_kanban_record_top">
                                    <div class="o_kanban_record_headings">
                                        <strong class="o_kanban_record_title">
                                            <field name="name"/>
                                        </strong>
                                        <strong class="o_kanban_record_subtitle">
                                            <field name="user_name"/>
                                        </strong>
                                        <strong class="o_kanban_record_subtitle">
                                            <field name="amount"/>
                                        </strong>
                                    </div>
                                </div>
                                <strong style="color:blue;">
                                    <field name="request_date"/>
                                </strong>
                                <br></br>
                                <strong style="color:green;">State /
                                    <field name="state"/>
                                </strong>

                            </div>
                        </t>
                    </templates>
                </kanban>
            </field>
        </record>

