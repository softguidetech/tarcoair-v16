<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="record_rule_analytic_account" model="ir.rule">
        <field name="name">Analytic Account Record Rule</field>
        <field name="model_id" ref="analytic.model_account_analytic_account"/>
        <field name="groups" eval="[(4, ref('analytic.group_analytic_accounting'))]"/>
        <field name="perm_read" eval="True"/>
        <field name="perm_write" eval="True"/>
        <field name="perm_create" eval="True"/>
        <field name="perm_unlink" eval="True"/>
        <field name="domain_force">[('id', 'in', user.analytic_account_ids.ids)]</field>
    </record>

    <record id="group_product_creation" model="res.groups">
        <field name="name">Product Creation Group</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>

    <record id="group_partner_creation" model="res.groups">
        <field name="name">Partner Creation Group</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>

    <record id="group_account_creation" model="res.groups">
        <field name="name">Account Creation Group</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>

